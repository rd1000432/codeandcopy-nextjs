{
  "name": "code-and-copy-nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint && npx stylelint \"**/*.scss\"",
    "lint:fix": "next lint --fix && npx stylelint \"**/*.scss\" --fix && npm run prettier:format",
    "lint:package": "npx sort-package-json",
    "lint:staged": "npx lint-staged",
    "prettier:check": "npx prettier --config .prettierrc '**/*.{ts,tsx}' --check --ignore-path .eslintignore",
    "prettier:format": "npx prettier --config .prettierrc '**/*.{ts,tsx}' --write --ignore-path .eslintignore",
    "dotenv-test": " dotenv cross-var \"echo STORYBLOK_SPACE_ID %STORYBLOK_SPACE_ID%\"",
    "generate-sb-types:mac": "cd src/storyblok/ && npx storyblok pull-components --space=326651 && npx storyblok-generate-ts source=components.326651.json target=component-types-sb"
  },
  "dependencies": {
    "@storyblok/react": "^4.4.0",
    "@svgr/webpack": "^8.1.0",
    "classnames": "^2.5.1",
    "dotenv-cli": "^8.0.0",
    "next": "15.1.7",
    "next-intl": "^3.26.5",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-fast-marquee": "^1.6.5",
    "reset-css": "^5.0.2",
    "storyblok-rich-text-react-renderer": "^2.9.2"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^20",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "cross-var": "^1.1.0",
    "eslint": "^9",
    "eslint-config-next": "15.1.7",
    "eslint-plugin-sonarjs": "^3.0.2",
    "postcss": "^8",
    "storyblok-generate-ts": "^2.1.0",
    "sass": "^1.85.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5",
    "prettier": "^3.1.1",
    "reset-css": "^5.0.2",
    "stylelint": "^16.3.1",
    "stylelint-config-css-modules": "^4.4.0",
    "stylelint-config-standard": "^34.0.0",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-order": "^6.0.4",
    "stylelint-scss": "^5.3.2",
    "stylelint-selector-bem-pattern": "^3.0.1"
  }
}
